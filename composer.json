{
  "name": "pittacusw/database",
  "description": "Generates a mysqldump for your Laravel site or restores it",
  "keywords": [
    "laravel",
    "generators",
    "seed",
    "artisan"
  ],
  "require": {
    "php": "^7.2|^8.0",
    "illuminate/support": "^7.0|^8.0",
    "laravel/framework": "^8.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-0": {
      "Pittacusw\\Database": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pittacusw\\Database\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pittacusw\\Database\\DatabaseServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
